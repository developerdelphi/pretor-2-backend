{
  "name": "pretor-backend",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "test": "jest --passWithNoTests --noStackTrace --runInBand --detectOpenHandles  --forceExit",
    "test:verbose": "jest --passWithNoTests --runInBand",
    "test:unit": "npm test -- --watch",
    "test:integration": "npm test -- --watch -c jest-integration-config.js",
    "test:staged": "npm test -- --findRelatedTests",
    "test:ci": "npm test -- --coverage",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "prepare": "husky install",
    "main": "tsc && node ./dist/main/api/main --trace-warnings",
    "build": "rimraf dist/* && tsc -p tsconfig.build.json",
    "up": "npm run build && docker-compose up",
    "down": "docker-compose down",
    "start": "node ./dist/main/api/main.js"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "express": "^4.18.1",
    "husky": "^8.0.1",
    "knex": "^2.3.0",
    "module-alias": "^2.2.2",
    "pg": "^8.8.0",
    "pg-promise": "^10.12.0",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.14",
    "@types/jest": "^29.0.1",
    "@types/module-alias": "^2.0.1",
    "@types/node": "^18.7.16",
    "@types/pg": "^8.6.5",
    "@types/rimraf": "^3.0.2",
    "@types/validator": "^13.7.6",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-standard-with-typescript": "^22.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "jest": "^29.0.3",
    "lint-staged": "^13.0.3",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.0.0",
    "typescript": "*"
  }
}
